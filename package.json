{
  "name": "@input-output-hk/atala-prism-wallet-sdk",
  "version": "0.0.1",
  "description": "PRISM typescript SDK",
  "main": "src/index.ts",
  "exports": {
    "./node": {
      "require": "./build/node/cjs/index.js",
      "import": "./build/node/es/index.js"
    },
    "./browser": {
      "require": "./build/browser/cjs/index.js",
      "import": "./build/browser/es/index.js"
    }
  },
  "files": [
    "src",
    "node",
    "browser",
    "index.ts",
    "apollo",
    "castor",
    "config",
    "domain",
    "mercury",
    "pluto",
    "pollux",
    "prism-agent",
    "peerdid"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/input-output-hk/atala-prism-wallet-sdk-ts.git",
    "directory": "@input-output-hk/atala-prism-wallet-sdk"
  },
  "scripts": {
    "build": "rm -rf build && npm run build:node && npm run build:node-esm && npm run build:browser && npm run build:browser-esm",
    "build:node": "rm -rf build/node/cjs && npx rollup -c rollup/node/rollup.cjs.js",
    "build:node-esm": "rm -rf build/node/es && npx rollup -c rollup/node/rollup.es.js",
    "build:browser": "rm -rf build/browser/cjs && npx rollup -c rollup/browser/rollup.cjs.js",
    "build:browser-esm": "rm -rf build/browser/es && npx rollup -c rollup/browser/rollup.es.js",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' npx nyc mocha -r ts-node/register 'tests/**/**/*.ts'",
    "test:watch": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' npx nyc mocha -r ts-node/register 'tests/**/*.ts' --watch-files tests/**/*.test.ts --extension ts --watch",
    "amtlr": "./node_modules/.bin/antlr4ts ./castor/parser/DIDAbnf.g4  -long-messages  && ./node_modules/.bin/antlr4ts  -long-messages ./castor/parser/DIDUrlAbnf.g4",
    "protos": "yarn protos:node && yarn protos:common",
    "protos:node": "protoc -Iprotos --proto_path=castor/protos --ts_out=castor/protos  castor/protos/node_models.proto",
    "protos:common": "protoc -Iprotos --proto_path=castor/protos --ts_out=castor/protos  castor/protos/common_models.proto",
    "coverage:check": "npx nyc check-coverage --lines 70 --branches 45 --functions 60 --statements 70"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@rollup/plugin-wasm": "^5.1.2",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/chai": "^4.3.4",
    "@types/elliptic": "^6.4.14",
    "@types/google-protobuf": "^3.15.6",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.14.2",
    "@types/sinon": "^10.0.13",
    "@types/sql.js": "^1.4.4",
    "@types/sqlite3": "^3.1.8",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "antlr4ts-cli": "^0.5.0-alpha.4",
    "chai": "^4.3.7",
    "eslint": "^8.35.0",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "puppeteer": "^19.7.2",
    "rollup": "^2.68.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-ignore": "^1.0.10",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.34.1",
    "run-all": "^1.0.1",
    "semantic-release": "19.0.5",
    "semantic-release-slack-bot": "^3.5.3",
    "sinon": "^15.0.1",
    "ts-node": "^10.8.1",
    "ts-protoc-gen": "^0.15.0",
    "typescript": "^4.9.5"
  },
  "keywords": [],
  "author": "",
  "license": "",
  "dependencies": {
    "@scure/bip39": "^1.1.1",
    "@sicpa_open_source/peer-did-ts": "^0.2.0",
    "@stablelib/sha256": "^1.0.1",
    "antlr4ts": "^0.5.0-alpha.4",
    "bn.js": "^5.2.1",
    "elliptic": "^6.5.4",
    "google-protobuf": "^3.21.2",
    "jose": "^4.12.2",
    "multiformats": "^9.9.0",
    "uuid": "^9.0.0"
  },
  "peerDependencies": {
    "sql.js": "^1.8.0",
    "sqlite3": "^5.1.4"
  }
}
