{
  "name": "@input-output-hk/atala-prism-wallet-sdk",
  "version": "0.0.1",
  "description": "PRISM typescript SDK",
  "main": "build/cjs/index.js",
  "browser": "build/cjs/index.js",
  "module": "build/es/index.js",
  "types": "build/types/index.d.ts",
  "files": [
    "build"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/input-output-hk/atala-prism-wallet-sdk-ts.git",
    "directory": "@input-output-hk/atala-prism-wallet-sdk"
  },
  "scripts": {
    "build": "tsc -b tsconfig.cjs.json tsconfig.esm.json tsconfig.types.json",
    "build:test": "tsc -b tsconfig.tests.cjs.json",
    "test": "npm run test:node && npm run test:browser",
    "test:node": "npx nyc node_modules/mocha/bin/_mocha -r ts-node/register --timeout 9999 --colors tests/**/*.ts",
    "test:browser": "npx karma start karma.conf.js",
    "amtlr": "./node_modules/.bin/antlr4ts ./castor/parser/DIDAbnf.g4  -long-messages  && ./node_modules/.bin/antlr4ts  -long-messages ./castor/parser/DIDUrlAbnf.g4",
    "protos": "yarn protos:node && yarn protos:common",
    "protos:node": "protoc -Iprotos --proto_path=castor/protos --ts_out=castor/protos  castor/protos/node_models.proto",
    "protos:common": "protoc -Iprotos --proto_path=castor/protos --ts_out=castor/protos  castor/protos/common_models.proto",
    "coverage:check": "npx nyc check-coverage --lines 70 --branches 45 --functions 60 --statements 70"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/antlr4": "^4.11.2",
    "@types/chai": "^4.3.4",
    "@types/elliptic": "^6.4.14",
    "@types/google-protobuf": "^3.15.6",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.14.2",
    "@types/sinon": "^10.0.13",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "antlr4": "^4.12.0",
    "antlr4ts-cli": "^0.5.0-alpha.4",
    "browserify": "^17.0.0",
    "chai": "^4.3.7",
    "crypto-browserify": "^3.12.0",
    "eslint": "^8.35.0",
    "esm": "^3.2.25",
    "karma": "^6.4.1",
    "karma-browserify": "^8.1.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-commonjs": "^1.0.0",
    "karma-headless-chrome-launcher": "^0.0.6",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.4.0",
    "karma-typescript": "^5.5.3",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "run-all": "^1.0.1",
    "semantic-release": "19.0.5",
    "semantic-release-slack-bot": "^3.5.3",
    "sinon": "^15.0.1",
    "ts-node": "^10.8.1",
    "ts-protoc-gen": "^0.15.0",
    "typescript": "^4.9.5",
    "watchify": "^4.0.0"
  },
  "keywords": [],
  "author": "",
  "license": "",
  "dependencies": {
    "@scure/bip39": "^1.1.1",
    "@stablelib/base64": "^1.0.1",
    "@stablelib/sha256": "^1.0.1",
    "antlr4ts": "^0.5.0-alpha.4",
    "bip32": "^4.0.0",
    "bn.js": "^5.2.1",
    "elliptic": "^6.5.4",
    "google-protobuf": "^3.21.2",
    "jose": "^4.12.2",
    "multiformats": "^9.9.0",
    "tiny-secp256k1": "^2.2.1",
    "uuid": "^9.0.0"
  }
}
