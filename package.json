{
	"name": "cryptribo",
	"version": "0.1.4",
	"description": "A ready to use library including the emurgo wasm for Cardano and a set of utilities to encrypt, decrypt, sign and verify anything using native wasm",
	"main": "src/index.ts",
	"exports": {
		"./node": {
			"require": "./build/node/cjs/index.js",
			"import": "./build/node/es/index.js"
		},
		"./browser": {
			"require": "./build/browser/cjs/index.js",
			"import": "./build/browser/es/index.js"
		}
	},
	"files": [
		"src",
		"node",
		"browser",
		"index.d.ts"
	],
	"types": "build/index.d.ts",
	"scripts": {
		"build": "rm -rf build && npm run build:types && npm run build:node && npm run build:node-esm && npm run build:browser && npm run build:browser-esm",
		"build:types": "npx tsc --emitDeclarationOnly true",
		"build:node": "rm -rf build/node/cjs && npx rollup -c config/node/rollup.cjs.js",
		"build:node-esm": "rm -rf build/node/es && npx rollup -c config/node/rollup.es.js",
		"build:browser": "rm -rf build/browser/cjs && npx rollup -c config/browser/rollup.cjs.js",
		"build:browser-esm": "rm -rf build/browser/es && npx rollup -c config/browser/rollup.es.js"
	},
	"devDependencies": {
		"@babel/core": "^7.7.2",
		"@babel/plugin-proposal-decorators": "^7.15.4",
		"@babel/plugin-proposal-export-default-from": "^7.14.5",
		"@babel/plugin-transform-runtime": "^7.15.0",
		"@babel/plugin-transform-typescript": "^7.15.4",
		"@babel/preset-env": "^7.7.1",
		"@babel/preset-typescript": "^7.15.0",
		"@babel/runtime-corejs3": "^7.15.4",
		"@input-output-hk/atala-prism-sdk": "1.4.1",
		"@rollup/plugin-commonjs": "^24.0.1",
		"@rollup/plugin-node-resolve": "^13.0.6",
		"@rollup/plugin-replace": "^3.0.0",
		"@rollup/plugin-wasm": "^5.1.2",
		"@types/elliptic": "^6.4.14",
		"@types/uuid": "^9.0.1",
		"arraybuffer-loader": "^1.0.8",
		"babel-loader": "^8.2.2",
		"babel-plugin-add-module-exports": "^1.0.4",
		"babel-plugin-nameof-js": "^0.0.2",
		"bn.js": "^5.2.1",
		"clean-webpack-plugin": "^4.0.0",
		"crypto": "^1.0.1",
		"crypto-browserify": "^3.12.0",
		"elliptic": "^6.5.4",
		"event": "^1.0.0",
		"file-loader": "^6.2.0",
		"npm-run-all": "^4.1.5",
		"path-browserify": "^1.0.1",
		"pbkdf2": "^3.1.2",
		"process": "^0.11.10",
		"puppeteer": "^1.13.0",
		"rollup": "^2.68.0",
		"rollup-plugin-babel": "^4.4.0",
		"rollup-plugin-cleanup": "^3.2.1",
		"rollup-plugin-copy": "^3.4.0",
		"rollup-plugin-delete": "^2.0.0",
		"rollup-plugin-dts": "^5.2.0",
		"rollup-plugin-ignore": "^1.0.10",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-typescript2": "^0.19.3",
		"run-all": "^1.0.1",
		"stream-browserify": "^3.0.0",
		"ts-node": "^10.2.1",
		"typescript": "^4.2.3",
		"util": "^0.12.4",
		"wasm-loader": "^1.3.0",
		"webpack": "5.37.0",
		"webpack-cli": "4.7.2",
		"webpack-filter-warnings-plugin": "^1.2.1"
	},
	"dependencies": {
		"apollo": "./package",
		"uuid": "^9.0.0"
	},
	"keywords": [],
